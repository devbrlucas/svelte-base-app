{
	"name": "svelte-base-app",
	"version": "1.2.38",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"svelte": "./dist/auth/index.js"
		},
		"./form": {
			"types": "./dist/form/index.d.ts",
			"svelte": "./dist/form/index.js"
		},
		"./filter": {
			"types": "./dist/filter/index.d.ts",
			"svelte": "./dist/filter/index.js"
		},
		"./service-worker": {
			"types": "./dist/service-worker/index.d.ts",
			"import": "./dist/service-worker/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"less": "^4.2.0",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@types/qrcode": "^1.5.5",
		"less": "^4.2.0",
		"publint": "^0.1.9",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.5.1",
		"qr-scanner": "^1.4.2",
		"qrcode": "^1.5.3"
	}
}
